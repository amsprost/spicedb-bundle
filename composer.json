{
    "name": "linkorb/spicedb-php-bundle",
    "description": "SpiceDB connector for Symfony apps",
    "license": "MIT",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "LinkORB\\AuthzedBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LinkORB\\AuthzedBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "LinkORB"
        }
    ],
    "require": {
        "php": ">=7.4",
        "linkorb/spicedb-php": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/dependency-injection": "^6.2",
        "symfony/config": "^6.2",
        "symfony/http-kernel": "^6.2",
        "symfony/yaml": "^6.2",
        "symfony/test-pack": "^1.0",
        "symfony/framework-bundle": "^6.0"
    }
}
